<UserControl x:Class="DownloadData.ErrorDownloading"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DownloadData"
             mc:Ignorable="d" 
             d:DesignHeight="320" d:DesignWidth="480">
    <Grid>
        <Button x:Name="ButtonBack" Content="Back" HorizontalAlignment="Left" Margin="80,240,0,0" VerticalAlignment="Top" Width="75" Click="ButtonBack_OnClick"/>
        <Label x:Name="LabelMessage" Content="Unable to download resource" HorizontalAlignment="Left" Margin="80,79,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
